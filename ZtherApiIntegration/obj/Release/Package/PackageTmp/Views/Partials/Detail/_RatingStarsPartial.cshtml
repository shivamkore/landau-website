@model int
<div class="stars">

    @for (int s = 1; s <= Math.Round((double)Model); s++)
    {
        <img class="star" srcset="public/images/detail/icons/star_filled.png" alt="" />
    }
    @for (int s = (int)Math.Max(Math.Round((double)Model), 1) + 1; s <= 5; s++)
    {
        <img class="star" srcset="public/images/detail/icons/star_empty.png" alt="" />
    }
    <meta itemprop="worstRating" content="1">
    <meta itemprop="ratingValue" content="3">
    <meta itemprop="bestRating" content="5">
</div>

